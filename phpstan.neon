includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:
    paths:
        - app
        - config
        - database
        - resources
        - routes
        - tests
        - public/index.php

    # The level 8 is the highest level
    level: 7

    ignoreErrors:
        - '#Parameter \#2 \$message of function abort expects string, array\|string\|null given.#'

    excludes_analyse:
        - routes/console.php

    checkMissingIterableValueType: false
